import os


os.system('cls')


lista = []


for num in range(1, 11):
    numeros = int(input(f'Insira o {num}ª valor: '))
    print('-' * 50)
    lista.append(numeros)

print('SAÍDA DOS ÍNDICES DOS NÚMEROS')
print('=' * 50)
for ind, numero in enumerate(lista, start=1):
    print(f'Índice: {ind} = Número: {numero}')
    print('-' * 50)